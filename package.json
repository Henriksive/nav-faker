{
  "name": "nav-faker",
  "version": "1.0.6",
  "description": "Faker for å generere fødselsnummer, navn, og mer.",
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc && npm run lint",
    "lint": "tslint lib/**/*.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "moment": "^2.22.2",
    "seedrandom": "^2.4.3",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/seedrandom": "^2.4.27",
    "jest": "^23.0.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.+(ts|tsx|js)"
    ],
    "modulePathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
